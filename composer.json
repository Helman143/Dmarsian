{
    "name": "capstone/martial-arts-school",
    "description": "Martial Arts School Management System",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-mysqli": "*",
        "ext-xml": "*",
        "ext-zip": "*",
        "aws/aws-sdk-php": "^3.368",
        "phpmailer/phpmailer": "^6.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Capstone\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Capstone\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-verbose": "phpunit --verbose"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
