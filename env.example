# Environment Variables Template
# Copy this file to .env and update with your actual values
# DO NOT commit .env to version control

# Database Configuration
DB_HOST=localhost
DB_USER=capstone_user
DB_PASS=your_database_password_here
DB_NAME=capstone_db
DB_PORT=3306

# SMTP2GO Email Configuration
SMTP2GO_API_KEY=your_smtp2go_api_key_here
SMTP2GO_SENDER_EMAIL=your_email@example.com
SMTP2GO_SENDER_NAME=D'Marsians Taekwondo Gym
ADMIN_BCC_EMAIL=admin@example.com

# Application Settings
APP_ENV=production
APP_DEBUG=false
APP_URL=https://yourdomain.com

# Timezone
TZ=Asia/Manila

# Security
SESSION_LIFETIME=7200







